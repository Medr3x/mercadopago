{
    "name": "medrex/mercadopago",
    "description": ":description",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "MEDERO LEONARDO",
            "email": "medero1025@gmail.com"
        }
    ],
    "homepage": "https://github.com/medrex/mercadopago",
    "keywords": ["Laravel", "mercadopago"],
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "medrex/mercadopago",
                "version":"master",
                "source": {
                    "url": "https://github.com/Medr3x/mercadopago.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        }
    ],
    "require": {
        "illuminate/support": "^5",
        "medrex/mercadopago": "dev-master"
    },  
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "medrex\\mercadopago\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "medrex\\mercadopago\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "medrex\\mercadopago\\mercadopagoServiceProvider"
            ],
            "aliases": {
                "mercadopago": "medrex\\mercadopago\\Facades\\mercadopago"
            }
        }
    }
}
